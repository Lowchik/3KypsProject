                <ListBox x:Name="ReasonList" Width="271" d:ItemsSource="{d:SampleData ItemCount=5}" Initialized="ReasonList_Initialized" MouseDoubleClick="ReasonList_MouseDoubleClick" IsEnabled="False"/>

